// Эту функцию следует указать в маркетинговой акции в качестве функции вычисления процентной скидки
function addDiscountPay()
{
  return discount;
}


function onSesOpen()
{

     var xhr = new ActiveXObject("MSXML2.XMLHTTP");
     var salt = Math.random();
     var ses_key = String(Math.random()*Math.random()*Math.random());

     xhr.open("POST", SERVER+"api/open_ses/"+ salt+"/", true);
     xhr.setRequestHeader("Content-Type", "application/x-www-form-urlencoded");
     var data = "key=" + ACCESS_KEY;
     data += "&session_key="+ ses_key;
     xhr.send(data);
     while(xhr.readyState != 4){
        frontol.actions.wait("Идет запрос к дисконтному серверу", 1);
     }
     if(xhr.status === 200) {
        try{
            resp = xhr.responseText;
            if (resp == ''1'')
            {
               frontol.userValues.set("user_session_key", ses_key);
               return;
            }

        }
        catch(e){
            frontol.actions.showError(e);
            return null;
        }
     }
     else if(xhr.status === 404)
     {
          frontol.actions.showError("Не удалось авторизовать Frontol. Обратитесь в тех-поддержку.");
     }


}

function onSesClose()
{

     var xhr = new ActiveXObject("MSXML2.XMLHTTP");
     var salt = Math.random();

     xhr.open("POST", SERVER+"api/close_ses/"+ salt+"/", true);
     xhr.setRequestHeader("Content-Type", "application/x-www-form-urlencoded");

            }

        }
        catch(e){
            frontol.actions.showError(e);
            return null;
        }
     }
     else if(xhr.status == 404)
     {
          frontol.actions.showError("Не удалось закрыть смену в ВТИ-ДИСКОНТ. Обратитесь в тех-поддержку.");
     }


}

// Эту функцию следует указать в маркетинговой акции в качестве функции вычисления рублевой скидки (бонусов)
function addBonusPay()
{
  var bonus_current;
  var doc_sum = frontol.currentDocument.totalSum;
  var bonus_limit = doc_sum * max_bonus_percentage / 100;
  if (bonus_limit >= bonus)
  {
     bonus_current = bonus;
  } else
  {
    bonus_current = bonus_limit;
  }

  if (bonus_current == 0)
  {
     return null;
  }


 if ( frontol.actions.showMessage("Вы можете оплатить бонусами " + bonus_current + ". Списать бонусы?", Button.YesNo, Icon.Question) == DialogResult.Yes)
 {
      payValue = frontol.actions.inputString("Введите сумму списания бонусов. Максимально: " + bonus_current, bonus_current, 10);
      if (payValue == null)
      {
       return null;
      }  else if  (parseFloat(payValue) > bonus_current)
      {
            frontol.actions.showMessage("Сумма списания превышает возможную сумму. Отмена скидок!");
            return null;
      }
      bonus = bonus - payValue;
      return parseFloat(payValue);


 } else
 {
   return null;
   }
}





function beforeClose()
{
         //Списываем бонусы
     if (payValue > 0 )
     {
      if (! remCardBonus(payValue))   // если списались
         {

                 // если не списались
             frontol.actions.wait("Сервер не отвечает. Списываем бонусы локально",2);
             if (! remCardBonusLocal(payValue))
             {
               for (var i=1; i< 11; i++ )
                {
                    frontol.currentDocument.stornoPayment(i);
                 }
                    frontol.actions.cancel();
             }

         }
       }
       frontol.currentDocument.userValues.set("bonus", bonus + payValue);
       frontol.currentDocument.userValues.set("bonus_reduce", payValue);
       frontol.currentDocument.userValues.set("discount", discount);
       frontol.currentDocument.userValues.set("accum", accumulation);
}


function afterClose()
{    if (frontol.currentDocument.cardValues != ""){
       // frontol.actions.showMessage( frontol.currentDocument.type.name);
        if (frontol.currentDocument.type.name ==  "ВОЗВРАТ")
        {
           var doc_sum = 0 - frontol.currentDocument.payment.sumInBaseCurrency;
        }
        else
        {
            var doc_sum = frontol.currentDocument.totalSum;
        }
      if ( ! addCardAccum(doc_sum)) {
          addCardAccumLocal(doc_sum);
      };
      }
}


function getParams()
{
     var xhr = new ActiveXObject("MSXML2.XMLHTTP");
     var salt = Math.random();
     xhr.open("POST", SERVER+"api/get_params/"+ salt+"/", true);
     xhr.setRequestHeader("Content-Type", "application/x-www-form-urlencoded");
     var data = "key=" + ACCESS_KEY;
     data += "&session_key=" + frontol.userValues.get("user_session_key");
     xhr.send(data);
     var plan = '''';
     while(xhr.readyState != 4){
        frontol.actions.wait("Идет запрос к дисконтному серверу", 1);
     }
     if(xhr.status == 200) {
        try{
            resp = xhr.responseText;
            if (plan == ''0'')
            {
               return;
            }
            var temp = resp.split('';'',1);
            max_bonus_percentage = parseFloat(temp[0]);
            return;

        }
        catch(e){
            frontol.actions.showError(e);
            return null;
        }
     }
     else if(xhr.status === 404)
     {
          frontol.actions.showError("Не удалось загрузить параметры!");
          return false;
     }
      else if(xhr.status == 403)
     {
          frontol.actions.showError("Не удалось авторизовать кассу на сервере. Возможно смена не открыта.");
          return false;
     }
      else if(xhr.status === 503)
     {
          frontol.actions.showError("Проверьте параметры доступа и действие подписки");
          return false;
     }
     else
     {
        frontol.actions.showError("Ошибка получения данных с дисконтного сервиса");
        return false;
     }
}



function getCard(Card)
{
      card_code = Card.value;

      if (frontol.currentDocument.cardValues != "")
      {
          frontol.actions.showError("Карта клиента уже введена! Воспользуйтесь кнопкой отмены введенных карт.");
          return null;
      }
     // if (! getDiscountPlan())
     // {
    //         return null;
    //  }

      payValue=0;
      bonus=0;
      discount=0;
      accumulation=0;
      var xhr = new ActiveXObject("MSXML2.XMLHTTP");
      var salt = Math.random();
     xhr.open("POST", SERVER+"api/cards/"+card_code+"_" + salt+"/", true);
     xhr.setRequestHeader("Content-Type", "application/x-www-form-urlencoded");
     var data = "key=" + ACCESS_KEY;
     data += "&session_key=" + frontol.userValues.get("user_session_key");
     xhr.send(data);
     var Disc = 0;
     while(xhr.readyState != 4){
        frontol.actions.wait("Идет запрос к дисконтному серверу", 1);
     }
     if(xhr.status === 200) {
        try{
            Disc = xhr.responseText;
            var temp = Disc.split(''#'',3);
            var msg = '''';

            bonus = parseFloat(temp[0]);
            discount = parseFloat(temp[1]);
            accumulation = parseFloat(temp[2]);

            getParams();

            if (bonus > 0)
            {
               msg= "Бонусов: "+bonus + ";";
            }
            if (discount >0)
            {
               msg +=  "Скидка: "+temp[1] + "%";
            }

            if (msg != '''')
            {
             frontol.actions.showMessage(msg, Icon.Asterisk);
            }


            return ;
        }
        catch(e){
            frontol.actions.showError(e);
            return null;
        }
     }
     else if(xhr.status === 404)
     {
          frontol.actions.showError("Карта " + card_code+" не найдена");
          return null;
     }
      else if(xhr.status == 403)
     {
          frontol.actions.showError("Не удалось авторизовать кассу на сервере. Возможно смена не открыта.");
          return false;
     }
      else if(xhr.status === 503)
     {
          frontol.actions.showError("Проверьте параметры доступа и действие подписки");
          return null;
     }
     else
     {
        frontol.actions.showMessage("Ошибка получения данных с дисконтного сервера. Проверьте подключение к интернету. БОНУСЫ И СКИДКИ НЕДОСТУПНЫ.");

        return;
     }


}
